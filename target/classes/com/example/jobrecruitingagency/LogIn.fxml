<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="442.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.jobrecruitingagency.LogInController">
   <children>
      <Label layoutX="136.0" layoutY="142.0" text="Phone Number:">
         <font>
            <Font name="Cambria" size="15.0" />
         </font>
      </Label>
      <Label layoutX="47.0" layoutY="152.0" />
      <Label layoutX="166.0" layoutY="183.0" text="Password:">
         <font>
            <Font name="Arial" size="15.0" />
         </font>
      </Label>
      <TextField fx:id="phoneNumberTF" layoutX="253.0" layoutY="139.0" promptText="Phone Number" />
      <PasswordField fx:id="passwordTF" layoutX="253.0" layoutY="179.0" promptText="Password" />
      <Button layoutX="215.0" layoutY="262.0" mnemonicParsing="false" onAction="#logInOA" prefHeight="26.0" prefWidth="170.0" style="-fx-background-color: #6495ED;" text="Log In" textFill="WHITE" />
      <Button layoutX="524.0" layoutY="33.0" mnemonicParsing="false" onAction="#backButtonOALogIn" style="-fx-border-color: gray;" text="Back" />
      <Label layoutX="141.0" layoutY="218.0" text="Account Type:">
         <font>
            <Font name="Arial" size="15.0" />
         </font>
      </Label>
      <ComboBox fx:id="accountTypeCBLogIn" layoutX="253.0" layoutY="214.0" prefWidth="150.0" promptText="Select type" />
      <Label fx:id="outputLabelLogIn" layoutX="124.0" layoutY="33.0" prefHeight="81.0" prefWidth="351.0" textFill="#bf0d0d">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
